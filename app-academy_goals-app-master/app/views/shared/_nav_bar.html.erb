<% if logged_in? %>
<p> You are logged in as: <h4> <%= current_user.username %> </h4> </p>
<p> <%= button_to "Log out", session_url, method: :delete %> </p>
<p> <%= link_to "My Goals", user_url(current_user) %> <p>
<% else %>
<p> <%= link_to "Sign In", new_session_url %> </p>
<p> <%= link_to "Sign Up", new_user_url %> </p>
<% end %>
